<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PercentView">
        <attr name="precent_circel_gravity">
            <flag name="left" value="0"/>
            <flag name="top" value="1"/>
            <flag name="center" value="2"/>
            <flag name="right" value="3"/>
            <flag name="bottom" value="4"/>
        </attr>
        <attr name="percent_circel_radius" format="dimension"/>
        <attr name="precent_circel_progress" format="integer"/>
        <attr name="precent_progress_color" format="color"/>
        <attr name="precent_background_color" format="color"/>
    </declare-styleable>
</resources>